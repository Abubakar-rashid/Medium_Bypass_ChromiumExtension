{
  "manifest_version": 3,
  "name": "Medium/Freedium Toggle",
  "version": "1.3",
  "content_scripts": [
    {
      "matches": [
        "*://*.medium.com/*",
        "*://medium.com/*",
        "*://freedium-mirror.cfd/*"
      ],
      "js": ["content.js"],
      "css": ["style.css"]
    }
  ],
  "permissions": ["tabs"]
}